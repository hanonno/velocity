<article>
    <div class="score" style="background-color: {{color}}">{{rank}}</div>
    <div class="content">
        <h1><a href="{{url}}" target="_blank">{{title}}</a></h1>
        <p class="meta">{{relative_date}} in <a href="/articles/popular/{{active_day}}/{{category}}">{{category}}</a> from <a href="{{source_url}}">{{source_name}}</a> via {{feed}}</p>
        <div class="description">

        </div>
    </div>
</article>