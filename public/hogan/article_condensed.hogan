<article class="nav {{type}} condensed" data-article-id="{{article_id}}" data-category="{{category}}">
    <header>
        {{#image_url}}
        <div class="media">
            <img src="{{square_image_url}}" />
        </div>
        {{/image_url}}    
        <h1>{{title}}</h1>
        <!-- <p>{{relative_date}} in <a>{{category}}</a> via {{source_name}}</p> -->
    </header>
    <section>
        <p>{{{description}}}</p>
    </section>
</article>